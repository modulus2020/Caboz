.navbar-nav a.main-links {
	background-image: linear-gradient(to right, #5ce1e6, #5ce1e6 50%, #fff 50%);
	background-size: 200% 100%;
	background-position: -100%;
	padding: 5px 0;
	position: relative;
	-webkit-background-clip: text;
	-webkit-text-fill-color: transparent;
	transition: all 0.3s ease-in-out;
	font-weight: 800;
	font-size: 20px;
	text-align: center;
}

.navbar-nav a.main-links:before {
	content: '';
	background: #79dbff;
	display: block;
	position: absolute;
	bottom: -3px;
	left: 0;
	width: 0;
	height: 3px;
	transition: all 0.3s ease-in-out;
}

.navbar-nav a.main-links:hover {
	background-position: 0;
}

